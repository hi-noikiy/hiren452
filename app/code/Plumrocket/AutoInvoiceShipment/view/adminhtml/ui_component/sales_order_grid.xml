<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="create_invoice">
                <settings>
                    <url path="prautoinvoiceshipment/order/massInvoice"/>
                    <type>create_invoice</type>
                    <label translate="true">Create Invoice</label>
                </settings>
            </action>
            <action name="create_shipment">
                <settings>
                    <url path="prautoinvoiceshipment/order/massShipment"/>
                    <type>create_shipment</type>
                    <label translate="true">Create Shipment</label>
                </settings>
            </action>
        </massaction>
    </listingToolbar>
</listing>
